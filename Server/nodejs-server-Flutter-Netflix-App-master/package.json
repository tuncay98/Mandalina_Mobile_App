{
  "name": "netflix-app",
  "version": "1.0.0",
  "description": "Prueba tecnica front para mercado libre argentina",
  "main": "./dist/index.js",
  "private": true,
  "scripts": {
    "lint:sass": "stylelint ./src/**/*.scss",
    "lint:ts": "tslint './src/**/*.ts*' --format stylish --force",
    "lint": "npm run lint:ts && npm run lint:sass",
    "test": "export NODE_ENV=tests && jest --coverage --config=configs/jest.json",
    "test-watch": "jest --watch",
    "cp-to-dist": "gulp copy-to-dist",
    "mkdir-dist": "rm -rf dist && mkdir dist",
    "tsc": "tsc -p ./configs/tsconfig-server.json",
    "nodemon": "nodemon --exec 'ts-node --project ./configs/tsconfig-server.json --cache-directory .tscache' ./src/index.ts",
    "build": "npm run mkdir-dist && npm run tsc && npm run cp-to-dist && rm -rf dist/app",
    "build-client": "webpack --config=configs/webpack/release.js",
    "dev": "export NODE_ENV=development && clear && npm run mkdir-dist && npm run cp-to-dist && npm run nodemon",
    "pr": "export NODE_ENV=release && clear && npm run mkdir-dist && npm run cp-to-dist && npm run nodemon",
    "start-pr": "export NODE_ENV=release && npm run build && npm run build-client && npm run start",
    "start-dev": "export NODE_ENV=development && npm run build && npm run build-client && npm run start",
    "start": "node ./dist/index.js",
    "client-start": "npm run client-dev",
    "client-dev": "webpack-dev-server --config=configs/webpack/development.js --content-base dist/",
    "apm-pr": "export NODE_ENV=release && npm run build && npm run build-client && pm2 start configs/pm2/release.json",
    "apm-dev": "export NODE_ENV=development && npm run build && npm run build-client && pm2 start configs/pm2/development.json"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "config": "^1.30.0",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-healthcheck": "^0.1.0",
    "history": "^4.7.2",
    "http-status-codes": "^1.3.0",
    "i18n": "^0.8.3",
    "i18next": "^11.3.3",
    "i18next-browser-languagedetector": "^2.2.0",
    "lodash": "^4.17.10",
    "mongoose": "^5.3.12",
    "mongoose-simple-random": "^0.4.1",
    "morgan": "^1.9.0",
    "pm2": "^2.10.4",
    "query-string": "^6.1.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-i18next": "^7.7.0",
    "react-lottie": "^1.2.3",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^5.1.1",
    "redux-thunk": "^2.3.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.35",
    "@types/config": "0.0.34",
    "@types/cors": "^2.8.3",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.11.1",
    "@types/express-serve-static-core": "^4.11.1",
    "@types/i18next-browser-languagedetector": "^2.0.1",
    "@types/jest": "^22.2.3",
    "@types/lodash": "^4.14.107",
    "@types/method-override": "0.0.31",
    "@types/mongoose": "^5.3.1",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.6.18",
    "@types/query-string": "^6.1.0",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "@types/react-helmet": "^5.0.6",
    "@types/react-i18next": "^7.6.1",
    "@types/react-redux": "^6.0.3",
    "@types/react-router": "^4.0.27",
    "@types/react-router-dom": "^4.2.7",
    "@types/redux-logger": "^3.0.6",
    "@types/supertest": "^2.0.4",
    "@types/winston": "^2.3.9",
